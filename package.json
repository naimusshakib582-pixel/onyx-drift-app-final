{
  "name": "onyx-drift-app-final",
  "version": "1.0.0",
  "description": "Fullstack application for OnyxDrift.",
  "type": "module",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "server": "nodemon server/index.js",
    "build": "npm install && cd client && npm install && npm run build",
    "dev": "concurrently \"npm run server\" \"cd client && npm run dev\""
  },
  "engines": {
    "node": ">=18.x"
  },
  "dependencies": {
    "express": "^4.21.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express-oauth2-jwt-bearer": "^1.6.0",
    "mongoose": "^8.0.0",
    "socket.io": "^4.8.1",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "nodemon": "^3.1.1",
    "concurrently": "^9.1.0",
    "vite": "^5.0.0"
  }
}